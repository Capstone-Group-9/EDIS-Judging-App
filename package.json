{
  "name": "edis",
  "version": "0.0.1",
  "description": "judging app",
  "productName": "judging app",
  "author": "Ethan Scoggins <esscog01@louisville.edu>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "watch": "npm-watch",
    "prism": "prism mock -d --cors -p 7080 openapi.yaml",
    "openapi": "openapi-generator-cli generate -g typescript-fetch -i openapi.yaml -o src/sdk/ -p withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models,legacyDiscriminatorBehavior=false"
  },
  "watch": {
    "openapi": {
      "patterns": [
        "openapi.yaml"
      ]
    },
    "prism": true,
    "dev": {
      "inherit": true,
      "patterns": [
        "package.json",
        "quasar.conf.js",
        "package-lock.json"
      ]
    }
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@quasar/app-vite": "^2.1.0",
    "@stoplight/prism-cli": "^5.12.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "npm-watch": "^0.13.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
